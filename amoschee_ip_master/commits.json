":[{"hash":"e8ac70eb917df1c0b72b50aa82d712a41f2c8684","isMergeCommit":false,"messageTitle":"Add Level-10 GUI","messageBody":"","tags":["Level-10"],"fileTypesAndContributionMap":{"gradle":{"insertions":1,"deletions":1},"fxml":{"insertions":41,"deletions":0},"java":{"insertions":209,"deletions":22}}},{"hash":"67b3fdf7fda562da651d8430f53a67bd0f814bd3","isMergeCommit":true,"messageTitle":"Merge branch \u0027branch-Level-10\u0027","messageBody":"","tags":["v1.0"],"fileTypesAndContributionMap":{}}]},{"date":"2024-09-08","commitResults":[{"hash":"cca39ebb55f9869dfa43926b5b88d7cbbd8d69a6","isMergeCommit":false,"messageTitle":"Add Level-10 GUI","messageBody":"","fileTypesAndContributionMap":{"gradle":{"insertions":1,"deletions":0},"java":{"insertions":18,"deletions":2}}},{"hash":"9e694d841d0471ab447cc29572f9cb8742bd5647","isMergeCommit":false,"messageTitle":"Refactor code for improved readability","messageBody":"","tags":["A-CodeQuality"],"fileTypesAndContributionMap":{"java":{"insertions":50,"deletions":52}}},{"hash":"dcec68c30e94b4b91885cbeabd1243a7f660faef","isMergeCommit":true,"messageTitle":"Merge pull request #1 from amoschee/branch-A-Assertions","messageBody":"","fileTypesAndContributionMap":{}},{"hash":"bb9e1278f7944dde5f928a1305c1a4ad21248e40","isMergeCommit":true,"messageTitle":"Merge pull request #2 from amoschee/branch-A-CodeQuality","messageBody":"Refactor code for improved readability","tags":["A-Assertions"],"fileTypesAndContributionMap":{}}]},{"date":"2024-09-14","commitResults":[{"hash":"b86131d3949aad7e51a76d35badaaac950b98cea","isMergeCommit":false,"messageTitle":"Add ArchiveCommand to archive tasks and enhance Storage with contentsToString method","messageBody":"This commit introduces a new ArchiveCommand class responsible for archiving the current tasks from Wiggly.txt into archive.txt within the same directory. This addition ensures that task data is preserved in an archived file format, providing better data management and recovery options.\n\nChanges:\n\nArchiveCommand Class:\n\nAdded the ArchiveCommand class, which implements the functionality to archive tasks.\nThe execute method in ArchiveCommand performs the following:\nReads the content from Wiggly.txt using the contentsToString method in the Storage class.\nWrites this content to archive.txt in the same directory.\nClears the current tasks from the task list to prepare for new entries.\nSaves the cleared task list back to storage.\nAdded error handling to manage potential IOException during file operations, which is thrown as a WigglyException to maintain consistency in error reporting.\nStorage Class Update:\n\nIntroduced a new method contentsToString in Storage.java.\nThe contentsToString method reads the entire content of the file associated with the Storage instance and returns it as a string. This method facilitates the ArchiveCommand class by providing a convenient way to retrieve file contents for archiving purposes.\nDetails:\n\nArchiveCommand Implementation:\n\nEnsures that if there is no content to archive, a user-friendly message is returned indicating that there is nothing to archive.\nCreates the archive.txt file if it does not already exist and writes the task content into it.\nProperly manages file resources by closing the file writer after operations to prevent resource leaks.\n\nStorage Class Enhancement:\n\nThe contentsToString method utilizes Files.readAllBytes to read the entire file content into a byte array and then converts it to a string. This approach simplifies the process of reading file content into the ArchiveCommand.\n\nImpact: This commit improves task management by allowing for task data to be archived, thus preserving historical task information. The new method in the Storage class enhances modularity and code reuse by providing a straightforward mechanism to access file content as a string.\n","tags":["BCD-Extension"],"fileTypesAndContributionMap":{"txt":{"insertions":0,"deletions":0},"java":{"insertions":101,"deletions":4}}},{"hash":"5cf2f0df49729dcae219bb3381bd10779637a307","isMergeCommit":true,"messageTitle":"Merge pull request #4 from amoschee/branch-BCD-Extension","messageBody":"Add ArchiveCommand","tags":["A-FullCommitMessage"],"fileTypesAndContributionMap":{}}]},{"date":"2024-09-20","commitResults":[{"hash":"84787f004fbd0065e304016032f17dd183ba72b6","isMergeCommit":false,"messageTitle":"Update README.md to include comprehensive user guide","messageBody":"","tags":["A-UserGuide"],"fileTypesAndContributionMap":{"java":{"insertions":23,"deletions":4},"md":{"insertions":123,"deletions":16}}},{"hash":"024064757b6b3f3cf9e35388f53f9ceb3d818155","isMergeCommit":true,"messageTitle":"Merge remote-tracking branch \u0027origin/branch-A-UserGuide\u0027","messageBody":"","fileTypesAndContributionMap":{}},{"hash":"b603caa3d0026f52256999d12236756c9fc8f797","isMergeCommit":false,"messageTitle":"Update README.md","messageBody":"","fileTypesAndContributionMap":{"md":{"insertions":9,"deletions":12}}},{"hash":"e04eaf95602c5fb5908b5c1e01cfb3b3086fa680","isMergeCommit":false,"messageTitle":"Update README.md","messageBody":"","fileTypesAndContributionMap":{"md":{"insertions":4,"deletions":4}}},{"hash":"84c551b61403435061a185d5417bb121b573cd0c","isMergeCommit":false,"messageTitle":"Update README.md","messageBody":"","tags":["v0.2"],"fileTypesAndContributionMap":{"md":{"insertions":1,"deletions":1}}},{"hash":"c9cde34cd7b2716186adc97779c23fc17fb40dd4","isMergeCommit":false,"messageTitle":"Update README.md","messageBody":"","fileTypesAndContributionMap":{"md":{"insertions":14,"deletions":13}}},{"hash":"9ffb0f9f5452d062b1d9669ab6de42fb2d94fdec","isMergeCommit":false,"messageTitle":"Update README.md","messageBody":"","fileTypesAndContributionMap":{"md":{"insertions":1,"deletions":1}}},{"hash":"944e40366bcdb883a441b9b6e2f8affde846ef9d","isMergeCommit":false,"messageTitle":"Update README.md","messageBody":"","fileTypesAndContributionMap":{"md":{"insertions":22,"deletions":22}}},{"hash":"c35488140a4f8c01519d5